      "phone": {
        "key": "电话|手机|号码",
        "rules": ["1[3-9][0-9]{3,9}", "[0-9]{6,9}", "[3-8][0-9]{4,9}", "0[0-9]{3}[3-8][0-9]{4,14}"]
      },
      "id":  {
        "key": "身份证|证件",
        "rules": ["[1-9][0-9]{5,18}"]
      },
      "address": {
        "key": "地址|住",
